{
    "Job":{
        "_table":"Jobs",
        "_primary":"JobID",
        "_columns":{
            "CompanyRef":{
                "type":"INTEGER"
            },
            "JobTitle":{
                "type":"TEXT"
            },
            "SummaryLocation":{
                "type":"TEXT"
            },
            "SalaryBenefits":{
                "type":"TEXT"
            },
            "Summary":{
                "type":"TEXT",
                "encoding":"Base64"
            },
            "Description":{
                "type":"TEXT",
                "encoding":"Base64"
            },
            "JobType":{
                "type":"TEXT"
            },
            "ApplicationURL":{
                "type":"TEXT"
            }             
        },
        "_relationships":{
            "JobCategories":{
                "_table":"Jobs_Categories",
                "_reference":{
                    "JobCategory":"CategoryID"
                }
            },
            "JobRegions":{
                "_table":"Jobs_Regions",
                "_reference":{
                    "JobRegion":"RegionID"
                }
            },
            "JobWorkTypes":{
                "_table":"Jobs_WorkTypes",
                "_reference":{
                    "JobWorkType":"WorkTypeID"
                }             
            }
        }
    },
    "Company":{
        "_table":"Companies",
        "_primary":"CompanyID",
        "_columns":{
            "CompanyRef":{
                "type":"TEXT"
            },
            "CompanyName":{
                "type":"TEXT"
            },
            "ProfileType":{
                "type":"TEXT"
            },
            "ProfileText":{
                "type":"TEXT",
                "encoding":"Base64"
            }            
        }

    },
    "Categories":{
        "_table":"Categories",
        "_primary":"CategoryID",
        "_columns":{
            "CategoryID":{
                "type":"INTEGER"
            },
            "CategoryName":{
                "type":"TEXT"
            }             
        }
    },
    "Regions":{
        "_table":"Regions",
        "_primary":"RegionID",
        "_columns":{
            "RegionID":{
                "type":"INTEGER"
            },
            "RegionName":{
                "type":"TEXT"
            }            
        }
    },
    "WorkTypes":{
        "_table":"WorkTypes",
        "_primary":"WorkTypeID",
        "_columns":{
            "WorkTypeID":{
                "type":"INTEGER"
            },
            "WorkTypeName":{
                "type":"TEXT"
            }            
        }
    }
    
}